{
  "author": "Florian Keller <github@floriankeller.de>",
  "bin": {
    "ordinem": "dist/cli.js"
  },
  "dependencies": {
    "commander": "2.19.0",
    "cosmiconfig": "5.1.0",
    "cron": "1.7.0",
    "fs-extra": "7.0.1",
    "handlebars": "4.1.1",
    "hapi": "18.1.0",
    "logdown": "3.2.7",
    "lowdb": "1.0.0",
    "moment": "2.24.0",
    "shortid": "2.2.14",
    "vision": "5.4.4"
  },
  "description": "A media library server and browser.",
  "devDependencies": {
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@types/cosmiconfig": "5.0.3",
    "@types/cron": "1.6.1",
    "@types/fs-extra": "5.0.5",
    "@types/hapi": "18.0.1",
    "@types/lowdb": "1.0.7",
    "@types/node": "11.11.3",
    "@types/shortid": "0.0.29",
    "@types/vision": "5.3.6",
    "cross-env": "5.2.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.3333"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "server",
    "media",
    "library",
    "browser"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ],
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "name": "ordinem",
  "repository": "https://github.com/ffflorian/ordinem.git",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json \"**/*.ts\"",
    "prettier": "prettier --ignore-path .gitignore \"**/*.ts\"",
    "release": "semantic-release",
    "start": "cross-env NODE_DEBUG=\"ordinem/*\" node dist/cli.js",
    "test": "exit 0"
  },
  "version": "1.0.0"
}
