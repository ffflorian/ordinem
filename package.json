{
  "author": "Florian Keller <github@floriankeller.de>",
  "bin": {
    "ordinem": "dist/cli.js"
  },
  "dependencies": {
    "commander": "2.20.0",
    "cosmiconfig": "5.2.0",
    "cron": "1.7.1",
    "cross-env": "5.2.0",
    "fs-extra": "7.0.1",
    "hapi": "18.1.0",
    "logdown": "3.2.8",
    "moment": "2.24.0"
  },
  "description": "A media library server and browser.",
  "devDependencies": {
    "@ffflorian/prettier-config": "0.0.3",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@types/cosmiconfig": "5.0.3",
    "@types/cron": "1.7.0",
    "@types/fs-extra": "5.0.5",
    "@types/hapi": "18.0.2",
    "@types/node": "12.0.0",
    "husky": "2.2.0",
    "lint-staged": "8.1.6",
    "prettier": "1.17.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.4.5"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "server",
    "media",
    "library",
    "browser"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "name": "ordinem",
  "prettier": "@ffflorian/prettier-config",
  "repository": "https://github.com/ffflorian/ordinem.git",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --project tsconfig.json",
    "prettier": "prettier \"**/*.{json,md}\"",
    "release": "semantic-release",
    "start": "cross-env NODE_DEBUG=\"ordinem/*\" node dist/cli.js",
    "test": "exit 0"
  },
  "version": "1.0.0"
}
